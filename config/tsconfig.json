{
	"compilerOptions": {
		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": true,
		"typeRoots": [
			"../node_modules/@types"
		]
	},
	"exclude": [
		"node_modules"
	]
}